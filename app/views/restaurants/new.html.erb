<div class="container">
<h2>Add a new restaurant</h2>
<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :category, collection:@categories %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <div class="btn btn-primary"><%= f.button :submit %></div>
<% end %>
</div>
